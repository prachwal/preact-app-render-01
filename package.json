{
  "name": "preact-app-render-01",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run api\"",
    "dev:frontend": "vite",
    "api": "cd api && nodemon --delay 100ms --ignore node_modules index.js",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@preact/signals": "^1.3.1",
    "fastify": "^5.6.2",
    "preact": "^10.28.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.10.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@vitest/ui": "^4.0.15",
    "concurrently": "^8.2.2",
    "jsdom": "^27.2.0",
    "nodemon": "^3.1.7",
    "typescript": "~5.9.3",
    "vite": "^7.2.6",
    "vitest": "^4.0.15"
  }
}
